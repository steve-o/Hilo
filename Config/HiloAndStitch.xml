		<synthetic name="CADINR="><divide/><leg>INR=X</leg><leg>CAD=X</leg></synthetic>
		<pair name="CADJPY="/>
		<synthetic name="CADKRW="><divide/><leg>KRW=X</leg><leg>CAD=X</leg></synthetic>
